{
  "name": "orchid/experiment",
  "description": "AB tests for Laravel",
  "type": "library",
  "license": "MIT",
  "homepage": "https://orchid.software",
  "support": {
    "issues": "https://github.com/orchidsoftware/platform/issues",
    "source": "https://github.com/orchidsoftware/platform",
    "docs": "https://github.com/orchidsoftware/platform/tree/master/docs",
    "email": "bliz48rus@gmail.com"
  },
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com",
      "homepage": "https://tabuna.github.io",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchid\\Experiment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchid\\Experiment\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "laravel/framework": "5.7.*||5.8.*||^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/php-code-coverage": "^6.0",
    "phpunit/phpunit": "~7.0",
    "predis/predis": "^1.1"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
